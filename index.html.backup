        function openAgentModal(agentType) {
            console.log('Opening modal for:', agentType); // Debug
            const agent = agentData[agentType];
            if (!agent) {
                console.error('Agent not found:', agentType);
                alert('Agent not found: ' + agentType);
                return;
            }

            const modal = document.getElementById('agent-modal');
            const modalTitle = document.getElementById('modal-title');
            const modalBody = document.getElementById('modal-body');

            if (!modal || !modalTitle || !modalBody) {
                console.error('Modal elements not found');
                alert('Modal elements not found in DOM');
                return;
            }

            // Set content
            modalTitle.textContent = agent.title;
            modalBody.innerHTML = `
                <div class="agent-details">
                    <div class="agent-role">${agent.role}</div>
                    ${agent.content}
                </div>
            `;

            // Show modal
            modal.classList.add('active');
            document.body.classList.add('modal-open');

            // Highlight code in modal
            if (typeof Prism !== 'undefined') {
                Prism.highlightAllUnder(modalBody);
            }
        }

        function closeModal() {
            const modal = document.getElementById('agent-modal');
            modal.classList.remove('active');
            document.body.classList.remove('modal-open');
        }

        // Close modal when clicking outside
        document.getElementById('agent-modal').addEventListener('click', (e) => {
            if (e.target.id === 'agent-modal' || e.target.classList.contains('modal-container')) {
                closeModal();
            }
        });

        // Close modal on Escape key
        document.addEventListener('keydown', (e) => {
            if (e.key === 'Escape') {
                closeModal();
            }
        });

        // Copy code from modal
        function copyModalCode(button) {
            const pre = button.nextElementSibling;
            const code = pre.textContent;

            navigator.clipboard.writeText(code).then(() => {
                button.textContent = 'Copied!';
                button.classList.add('copied');
                setTimeout(() => {
                    button.textContent = 'Copy';
                    button.classList.remove('copied');
                }, 2000);
            }).catch(err => {
                console.error('Failed to copy:', err);
            });
        }

        // Make function available globally
        window.openAgentModal = openAgentModal;
        window.closeModal = closeModal;
        window.copyModalCode = copyModalCode;

        // Initialize Prism for code highlighting
        if (typeof Prism !== 'undefined') {
            Prism.highlightAll();
        }

        // Attach event listeners to agent cards after DOM loads
        document.addEventListener('DOMContentLoaded', function() {
            console.log('DOM loaded, attaching event listeners...');

            // Find all agent cards and attach click handlers
            const agentCards = document.querySelectorAll('.agent-card[data-agent]');
            console.log('Found agent cards:', agentCards.length);

            agentCards.forEach(card => {
                const agentType = card.getAttribute('data-agent');
                card.addEventListener('click', function() {
                    console.log('Card clicked:', agentType);
                    openAgentModal(agentType);
                });
            });

            // Also handle cards with onclick attributes (backup)
            document.querySelectorAll('.agent-card').forEach(card => {
                if (!card.hasAttribute('data-agent')) {
                    // This card uses onclick, make sure function is available
                    card.style.cursor = 'pointer';
                }
            });
        });
    </script>
